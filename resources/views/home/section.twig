<h1>{{ sectionName }}</h1>
{% for story in stories %}
  <a href="/{{ story['slug'] }}">
    <div class="storyWrapper">
      <h3>{{ story["headline"] }}</h3>
      <p>{{ story["summary"] }}</p>
    </div>
    <div class="heroImageWrapper">
      <img src="{{focusedImageUrl(story['hero-image-s3-key'], [16,9], story['hero-image-metadata'], {w: 350})}}"/>
    </div>
  </a>
{% endfor %}
<div class="sectionpage-articles ">
      <div class="load-more-results list-article">
      </div>
      <div class="load-more-button load-article-container">
        {% include "load_more_stories" %}
      </div>
</div>
<script type="text/javascript">
  var storyTemplate = "list_articles" ;
  var api ='/api/v1/stories?fields=sections,hero-image-s3-key,headline,author-name,summary,first-published-at,slug&section=' + '{{section_engname}}';
  qtReady.push(function() { app.quintypeLoadMore($(".sectionpage-articles"), {limit: 7}, 8 ,api,storyTemplate);
   });
</script>

