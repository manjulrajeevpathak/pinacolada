<div class="social-links">
  <a class="fb-xfbml-parse-ignore facebook social-share" target="_blank" href="https://www.facebook.com/sharer/sharer.php?u={{ url }}" data-social-media-type="facebook" data-social-media-share-url="{{ url }}" data-mobile-iframe="true">
    {% include "story/social_icons/facebook_svg_icon" %}
  </a>
  {% set articleHeadline = headline|trim %}
  <a class="twitter social-share" target="_blank" data-social-media-type="twitter"
     href="https://twitter.com/intent/tweet?url={{ url }}&text={{ articleHeadline|url_encode  }}" data-social-media-share-url="{{ url }}">
     {% include "story/social_icons/twitter_svg_icon" %}
  </a>
  <a class="gplus social-share" target="_blank" data-social-media-type="google"
     href="https://plus.google.com/share?url={{ url }}" data-social-media-share-url="{{ url }}">
     {% include "story/social_icons/gplus_svg_icon" %}
  </a>
  <a class="linkedin social-share" target="_blank" data-social-media-type="linkedin"
     href="https://www.linkedin.com/shareArticle?mini=true&url={{ url }}" data-social-media-share-url="{{ url }}">
     {% include "story/social_icons/linkedin_svg_icon" %}
  </a>
  <a class="whatsapp social-share" target="_blank" data-social-media-type="whatsapp"
     href="whatsapp://send?text={{ url }}" data-social-media-share-url="{{ url }}">
     {% include "story/social_icons/whatsapp_svg_icon" %}
  </a>
</div>
